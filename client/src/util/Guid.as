package rivalsEngine.util {			public class Guid{						var letterArray : Array = ["a","b","c","d","e","f","g","h","j","k","l","m","n","o","p","q","r","s","t","u","v","z"];				public function Guid(){					    var myNum = new Random().range(0,letterArray.length-1) ;									trace("HIHIHIHIHI RANDOM "  + myNum);		}				public function getGuid( inLength : String ):String{						var tempGui : String = "";									for ( var i =0; i< inLength ; i++ ){				var myLetter : String;				if (getTrueFalse() ){										myLetter= letterArray[ new Random().range(0,letterArray.length-1) ];									}else{														myLetter= String( new Random().range(0,9)) ;									}								tempGui = String(myLetter ) + String(tempGui);			}			return tempGui;		}	function getTrueFalse() :Boolean{				var myNum = new Random().range(0,1) ;				if ( myNum == 0 ){			return true;		}else{			return false;		}			}			}}